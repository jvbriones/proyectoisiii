<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="CentroMedico.Turno">
        <id name="Id" column="ID_TURNO" type="integer">
            <generator class="native" />
        </id>
        <property name="Fecha"/>
        <property name="Hora"/>

        <many-to-one name="PersonalMedico" class="CentroMedico.PersonalMedico" column="DNI_PERSONALMEDICO" cascade="all" not-null="true" lazy="true"/>
  </class>

</hibernate-mapping>

