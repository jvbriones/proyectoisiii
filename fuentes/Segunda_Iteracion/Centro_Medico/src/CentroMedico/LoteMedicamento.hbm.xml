<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="CentroMedico.MedicamentoRecetado">
        <id name="CodBarras" column="CODBARRAS" type="string">
            <generator class="assigned" />
        </id>
        <property name="Existencias"/>
        <property name="FechaCaducidad"/>
        
        <many-to-one class="CentroMedico.Medicamento" name="Medicamento" lazy="true"/>
        
  </class>
</hibernate-mapping>

